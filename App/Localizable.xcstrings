    "InformationContribution" : {
      "localizations" : {
        "el" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Αν θέλεις να συνεισφέρεις με υλικό, να καταθέσεις προτάσεις βελτίωσης της ιστοσελίδας ή της εφαρμογής, καθώς και για επισημάνσεις λαθών, θα χαρώ να επικοινωνήσεις μαζί μου."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "If you'd like to contribute with material, make suggestions for improving the website or the app, and highlight issues, I'd be happy to hear from you."
          }
        }
      }
    },
    "InformationDescription" : {
      "localizations" : {
        "el" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Η παρούσα εφαρμογή συγκεντρώνει στη συσκευή σου στίχους και συγχορδίες ορθόδοξων χριστιανικών τραγουδιών που αγαπήσαμε στα κατηχητικά και τις κατασκηνώσεις."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "This app brings to your device lyrics and chords of orthodox christian songs that we have loved in catechism and camps."
          }
        }
      }
    },
    "InformationDeveloper" : {
      "localizations" : {
        "el" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Κωνσταντίνος Ρακτιβάν"
