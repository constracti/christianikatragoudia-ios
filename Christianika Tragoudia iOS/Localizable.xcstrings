    "InformationDescription" : {
      "localizations" : {
        "el" : {
          "stringUnit" : {
            "state" : "needs_review",
            "value" : "Η παρούσα εφαρμογή συγκεντρώνει στη συσκευή σου στίχους και συγχορδίες ορθόδοξων χριστιανικών τραγουδιών που αγαπήσαμε στα κατηχητικά και τις κατασκηνώσεις."
          }
        },
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "This app brings to your device lyrics and chords of orthodox christian songs that we have loved in catechism and camps."
          }
